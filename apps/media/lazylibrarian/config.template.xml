<Config>
  <LogLevel>info</LogLevel>
  <UrlBase></UrlBase>
  <UpdateMechanism>Docker</UpdateMechanism>
  <BindAddress>*</BindAddress>
  <Port>5299</Port>
  <SslPort>5300</SslPort>
  <EnableSsl>False</EnableSsl>
  <LaunchBrowser>True</LaunchBrowser>
  <ApiKey>${LAZYLIBRARIAN_API_KEY}</ApiKey>
  <AuthenticationMethod>External</AuthenticationMethod>
  <Branch>master</Branch>
  <SslCertPath></SslCertPath>
  <SslCertPassword></SslCertPassword>
  <AuthenticationRequired>DisabledForLocalAddresses</AuthenticationRequired>
  <InstanceName>lazylibrarian</InstanceName>
  <PostgresUser>lazylibrarian</PostgresUser>
  <PostgresPassword>lazylibrarian</PostgresPassword>
  <PostgresPort>5432</PostgresPort>
  <PostgresHost>192.168.1.11</PostgresHost>
  <PostgresMainDb>lazylibrarian-main</PostgresMainDb>
  <PostgresLogDb>lazylibrarian-logs</PostgresLogDb>
</Config>
